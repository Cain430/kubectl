cat <<EOF | kubectl apply -f -
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: deno-rest-api-ingress
  annotations:
    nginx.ingress.kubernetes.io/rewrite-target: /
spec:
  rules:
  - host: http://127.0.0.1:5000  #kan njmsh flask jarbtha w atytha adminastrator privelage khidmt ama maamltsh deployment donc tnjm tbd (yourapidoamain.com)
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: deno-rest-api-service
            port:
              number: 80
EOF
